    "@graphql-codegen/typescript-react-query": "^3.5.9",
    "@prisma/client": "3.13.0",
    "apollo-server-micro": "^3.6.7",
    "graphql": "^16.4.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "12.1.5",
    "nexus": "^1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^17.0.30",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/uuid": "^8.3.4",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "prisma": "^3.13.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
